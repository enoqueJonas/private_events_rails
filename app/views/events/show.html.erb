<h2> <%= @event.title %> </h2>
<p>Location: <%= @event.location %> </p>
<ul>
    <% @attendees.each do |attendee| %>
        <li> ID: <%= attendee.id %>, Email: <%= attendee.email %> </li>
    <% end %>
</ul>
<%= form_with url: user_event_attendances_path(user_id: current_user.id, event_id: @event.id), method: :post do |form| %>
    <%= form.submit "Attend Event" %>
<% end %>
