<div>
    <%= form_with model: [@user, @event], url: user_events_path do |form| %>
        <%= form.label :title %>
        <%= form.text_field :title %>
        <%= form.label :location %>
        <%= form.text_field :location %>
        <%= form.label :date %>
        <%= form.date_field :date %>
        <%= form.submit %>
    <% end %>
</div>